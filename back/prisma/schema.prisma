generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Client {
  id String @id @default(uuid())
}

model Barber {
  id String @id @default(uuid())
}

model Services {
  id Int @id @default(autoincrement())
}

model Schedule {
  id Int @id @default(autoincrement())
}
